

drug_id = sint.get_input_from(0)
threshold = sint.get_input_from(0)


row = 7
col = 4
table = sint.Matrix(row, col)

for x in table:
    x.input_from(1)


print_ln("need supervised entry:")

for i in range(row):
    entry = table[i]
    need_supervised = (entry[1] == drug_id) * (entry[2] > threshold)
    entry = entry * need_supervised + (1 - need_supervised) * -1
    plain_text = entry.reveal()
    @if_(plain_text[0] >= 0)
    def _():
        print_ln_to(0, '%s', plain_text)
